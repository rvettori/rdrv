<div class="field form-inline">
  <div class="form-group">    
    <%= f.attribute_fields do |a| %>
      <%= a.attribute_select({associations: [:zipcode, :occupation]}, {class: 'form-control'} )%>
    <% end %>

    <%= f.predicate_select({},{class:'form-control'}) %>

    <%= f.value_fields do |v| %>
      <%= v.text_field :value, {class: 'form-control'} %>
    <% end %>

    <a href="#" class="remove_fields btn btn-default"><span class="glyphicon glyphicon-remove text-danger" aria-hidden="true"></span></a>
  </div>
</div>